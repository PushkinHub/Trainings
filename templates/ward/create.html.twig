{% extends 'base.html.twig' %}

{% block title %}Ward create{% endblock %}
{% block content_title %}Ward create{% endblock %}
{% block content %}
<style>
    .example-wrapper {
        margin: 1em auto;
        max-width: 800px;
        width: 95%;
        font: 18px/1.5 sans-serif;
    }

    .example-wrapper code {
        background: #F5F5F5;
        padding: 2px 6px;
    }
</style>

{% form_theme form 'bootstrap_4_layout.html.twig' %}
<form method="post">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button class="btn btn-lg btn-primary" type="submit">
        Sign in
    </button>
    {{ form_end(form) }}
</form>
{% endblock %}
